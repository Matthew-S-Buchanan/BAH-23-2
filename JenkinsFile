pipeline {
    agent any
    stages {
        stage("Clean Up"){
            steps {
                deleteDir()
            }
        }    
        stage("Pull HTML from Github Repo") {
            steps {
                sh "git clone https://github.com/Matthew-S-Buchanan/BAH-23-2.git"
            }
        }
        stage("Push file to web instance") {
            steps {
                sh "scp -r -v -o StrictHostKeyChecking=no -i /var/lib/jenkins/.ssh/FinalKey.pem /var/lib/jenkins/workspace/EKS/BAH-23-2/Web*/* ec2-user@54.227.128.131:/var/www/html"
            }
        }
    }
}
